{
    "name": "go-types",
    "description": "Type-checking utilities for JavaScript",
    "author": "Michael Ko",
    "email": "koyote130708@gmail.com",
    "version": "1.3.1",
    "license": "MIT",
    "keywords": [
        "type checking",
        "type-checking",
        "types",
        "primitives",
        "objects"
    ],
    "homepage": "https://github.com/koyote130708/go-types",
    "repository": {
        "type": "git",
        "url": "https://github.com/koyote130708/go-types.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "npm run bundle & npm run minify",
        "bundle": "webpack build --mode=none --progress --entry ./index.js -o ./dist --output-filename go-types.js --output-library-name=Types --output-library-type=umd --output-global-object=this",
        "minify": "uglifyjs ./dist/go-types.js -c -m --source-map -o ./dist/go-types.min.js",
        "test": "mocha -u tdd test/*.js",
        "coverage": "c8 --clean npm run test",
        "docs": "documentation readme index.js --section=Documentation",
        "docs:html": "documentation build index.js -f html -o docs"
    },
    "devDependencies": {
        "c8": "^7.12.0",
        "chai": "^4.3.6",
        "documentation": "^14.0.0",
        "mocha": "^10.0.0",
        "uglify-js": "^3.17.3",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "engine": "node >= 0.10"
}
